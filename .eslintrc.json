{
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier",
        "prettier/@typescript-eslint"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": ["prettier", "@typescript-eslint"],
    "env": {
        "browser": true
    },
    "rules": {
        "prettier/prettier": 1,
        "jsx-boolean-value": [
            true,
            "never"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "no-console": 0
    }
}